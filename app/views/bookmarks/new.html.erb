<div class="form">
  <h2>Add New Bookmark</h2>

  <%= simple_form_for [@list, @bookmark] do |f| %>
    <%= f.association :movie %>
    <%= f.input :comment %>
    <%= f.submit class: "btn btn-dark" %>
  <% end %>
</div>

  <div class="d-flex justify-content-center">
    <%= link_to :back, class: "btn btn-dark" do %>
      <i data-feather="arrow-left-circle"></i>
    <%end %>
  </div>
