<div class="form">
  <h2>Create a New List</h2>

  <%= simple_form_for @list do |f| %>
    <%= f.input :name %>
    <%= f.submit class: "btn btn-dark" %>
  <% end %>
</div>

<div class="d-flex justify-content-center">
  <%= link_to :back, class: "btn btn-dark" do %>
    <i data-feather="arrow-left-circle"></i>
  <% end %>
</div>
